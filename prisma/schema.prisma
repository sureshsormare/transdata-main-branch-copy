generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "mongodb"
  url       = env("MONGODB_URI")
  directUrl = env("DIRECT_DATABASE_URL")
}

enum SubscriptionType {
  FREE
  PAID
}

enum SubscriptionPlan {
  TRIAL
  STARTER
  GROWTH
  SCALE
}

enum SubscriptionStatus {
  ACTIVE
  INACTIVE
}

model contactForm {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  name         String
  email        String
  organization String?
  contact      String?
  message      String?
  createdAt    DateTime @default(now())
}

model export_india_trades {
  id                     String  @id @default(auto()) @map("_id") @db.ObjectId
  Chapter                String  @map("CHAPTER")
  Buyer_Name             String  @map("CONSIGNEE")
  Buyer_Address          String  @map("CONSINEE_ADDRESS")
  Buyer_Contact_No       String? @map("CONTACT_NO")
  CUSH                   String?
  Country_of_Destination String  @map("Country")
  Invoice_Currency       String  @map("Currency")
  DRAWBACK               String
  Supplier_Name          String  @map("EXPORTER")
  E_MAIL_ID              String
  Exchange_Rate_USD      String  @map("Exchange_Rate_(USD)")
  Supplier_Address       String? @map("Exporter_Address")
  Supplier_City_State    String? @map("Exporter_City_State")
  Supplier_PIN           String? @map("Exporter_PIN")
  Incoterm               String  @map("FOB")
  IEC                    String
  INVOICE_NO             String
  INVOICE_SERIAL_NUMBER  String
  Product_Description    String  @map("ITEM")
  ITEM_NUMBER            String
  ITEM_RATE              String
  MODE                   String
  MONTH                  String
  PORT_CD                String
  PORT_CODE              String
  QUANTITY               String
  HS_Code                String  @map("RITC")
  Shipping_Bill_Date     String  @map("Shipping Bill Date")
  Shipping_Bill_No       String  @map("Shipping Bill No")
  State                  String
  Total_Amount_In_Fc     String  @map("Total Amount in Fc")
  Total_Value_In_USD     String  @map("Total_Value_IN_USD_(Exchange)")
  UQC                    String
  Unit_Value_In_INR      String  @map("Unit Value in INR")
  Unit_Rate_In_USD       String  @map("Unit_Rate_In_USD_(Exchange)")
  YEAR                   String
  desc                   Json?
  Port_of_Origin         String  @map("location")

  @@index([desc], map: "desc_1_autocreated")
}

model import_india_trades {
  id                        String  @id @default(auto()) @map("_id") @db.ObjectId
  A_Group                   String
  Assess_Value_In_INR       String
  BE_Type                   String
  Shipping_Bill_Date        String  @map("Bill of Entry Date")
  Shipping_Bill_No          String  @map("Bill of Entry No")
  CHA_Number                String  @map("CHA Number")
  CHA_Name                  String
  CUSH                      String?
  Chapter                   String  @map("Chapter")
  City                      String? @map("City ")
  Contact_No                String? @map("Contact No")
  Duty                      String
  Email                     String?
  Exchange_Rate             String  @map("Exchange Rate")
  Foreign_Port              String  @map("Foreign Port")
  HS_Code                   String  @map("HS Code")
  INDIAN_Port               String  @map("INDIAN Port")
  INVOICE_CURRENCY          String
  IEC                       String  @map("Importer ID")
  Buyer_Address             String? @map("Importer_Address")
  Buyer_City_State          String? @map("Importer_City_State")
  Buyer_Name                String  @map("Importer_Name")
  Invoice_No                String  @map("Invoice No")
  Item_Number               String  @map("Item Number")
  Country_of_Origin         String  @map("Origin Country")
  PIN_Code                  String? @map("PIN Code")
  Port_code                 String  @map("Port code")
  Product_Description       String  @map("Product Discription")
  Quantity                  String
  State                     String?
  Supplier_Address          String  @map("Supplier Address")
  Supplier_Name             String  @map("Supplier Name")
  Total_Amount_In_Fc        String  @map("Total Value In FC")
  Total_Value_In_USD        String  @map("Total Value USD (Exchange)")
  UQC                       String  @map("Unit Quantity")
  Unit_Rate_In_USD          String  @map("Unit Rate in USD")
  Unit_value_as_per_Invoice String  @map("Unit value as per Invoice")
  Unit_Value_In_INR         String  @map("Unit_Price in INR")
}

model uploaded_data_files {
  id            String @id @default(auto()) @map("_id") @db.ObjectId
  file_name     String
  file_type     String
  no_of_records String
  upload_date   String
}

model User {
  id            String          @id @default(auto()) @map("_id") @db.ObjectId
  name          String?
  email         String          @unique
  emailVerified DateTime?
  image         String?
  password      String
  designation   String?
  companyName   String?
  accounts      Account?
  sessions      Session[]
  // Optional for WebAuthn support
  Authenticator Authenticator[]

  createdAt   DateTime     @default(now())
  updatedAt   DateTime     @updatedAt
  userProfile UserProfile?
}

model UserProfile {
  id                          String             @id @default(auto()) @map("_id") @db.ObjectId
  userId                      String             @db.ObjectId
  role                        String?            @default("user")
  subscription_type           SubscriptionType   @default(FREE)
  subscription_plan           SubscriptionPlan   @default(TRIAL)
  subscription_status         SubscriptionStatus @default(INACTIVE)
  subscription_start_date     DateTime?
  subscription_end_date       DateTime?
  subscription_renewal_date   DateTime?
  subscription_renewal_status String?            @default("ACTIVE")
  createdAt                   DateTime           @default(now())
  updatedAt                   DateTime           @updatedAt

  user User @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([userId])
}

model Account {
  id                String  @id @default(auto()) @map("_id") @db.ObjectId
  userId            String  @unique @db.ObjectId
  type              String
  provider          String
  providerAccountId String
  refresh_token     String? @db.String
  access_token      String? @db.String
  expires_at        Int?
  token_type        String?
  scope             String?
  id_token          String? @db.String
  session_state     String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  user User @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([provider, providerAccountId])
}

model Session {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  sessionToken String   @unique
  userId       String   @db.ObjectId
  expires      DateTime
  user         User     @relation(fields: [userId], references: [id], onDelete: Cascade)

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model VerificationToken {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  identifier String
  token      String
  expires    DateTime

  @@unique([identifier, token])
}

// Optional for WebAuthn support
model Authenticator {
  credentialID         String  @id @map("_id")
  userId               String  @db.ObjectId
  providerAccountId    String
  credentialPublicKey  String
  counter              Int
  credentialDeviceType String
  credentialBackedUp   Boolean
  transports           String?

  user User @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([userId, credentialID])
}

